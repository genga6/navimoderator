{
    "manifest_version": 3,
    "name": "Navi Moderator",
    "version": "1.0",
    "description": "Authenticate and link a moderator account with the tool.",
    "permissions": [
      "identity",
      "storage",
      "activeTab",
      "scripting"
    ],
    "host_permissions": [
      "https://www.googleapis.com/*"
    ],
    "web_accessible_resources": [
      {
        "resources": ["config.js"],
        "matches": ["<all_urls>"]
      }
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
    
  }
  