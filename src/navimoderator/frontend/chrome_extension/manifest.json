{
    "manifest_version": 3,
    "name": "Navi Moderator",
    "version": "1.0",
    "description": "Authenticate and link a moderator account with the tool.",
    "permissions": [
      "identity",
      "storage",
      "activeTab",
      "scripting"
    ],
    "host_permissions": [
      "https://www.googleapis.com/*"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "flutter/index.html",
      "default_icon": "icon.png"
    }
  }
  